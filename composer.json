{
    "name": "symsonte/js-api",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "symsonte/framework": "@dev",
        "phpdocumentor/reflection-docblock": "~4.3"
    },
    "require-dev": {
        "symfony/debug": "~3"
    },
    "autoload": {
        "psr-4": {
            "Symsonte\\JsApi\\": [
                "src"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symsonte\\JsApi\\Play\\": [
                "play/setup",
                "play/src"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://php.yosmy.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev"
}
